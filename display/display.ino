void setup() {
  pinMode(BUTTON_PIN, INPUT_PULLUP);
  FastLED.addLeds<WS2812B, LED_PIN, GRB>(leds, LED_COUNT);
  FastLED.setBrightness(10);
}

void clearStrip() {
  FastLED.clear();
  FastLED.show();
}

void buttonLogic() {
  unsigned long currentTime = millis();

  int buttonState = digitalRead(BUTTON_PIN);

  if (buttonState == HIGH && lastButtonState == LOW && (currentTime - lastButtonPressTime > debounceDelay)) {
    lastButtonPressTime = currentTime;
    animationMode = (animationMode + 1) % MODE_NUM;
    clearStrip();
  }

  lastButtonState = buttonState;
}

void drawFrom2DArray(const uint32_t arr[36][36], int row) {
  for (int c = 0; c < LED_COUNT; c++) {
    uint32_t color = pgm_read_dword_near(&(arr[row][c]));
    leds[c] = color;
  }
}

void drawFire() {
  for(int i = 0; i < LED_COUNT; i++) {
    int flicker = random(0, 130);  // Random flickering range
    int r = constrain(190 - flicker, 0, 255);
    int g = constrain(100 - flicker, 0, 255);
    int b = constrain(0 - flicker, 0, 255);
    leds[i] = CRGB(r, g, b);
  }
}

